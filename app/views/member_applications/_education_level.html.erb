<%= f.form_group do %>
  <%= f.check_box :less_hs, label: "Less than High School", inline: true %>

  <%= f.check_box :some_hs, label: "Some High School", inline: true %>

  <%= f.check_box :ged, label: "GED",
    inline: true %>

  <%= f.check_box :hs_diploma, label: "High School Diploma", inline: true %>

  <%= f.check_box :trade_school, label: "Trade School", inline: true %>

  <%= f.check_box :some_college, label: "Some College", inline: true %>

  <%= f.check_box :assoc_degree, label: "Associate's Degree", inline: true %>

  <%= f.check_box :bachelors_degree, label: "Bachelor's Degree", inline: true %>

  <%= f.check_box :some_grad_work, label: "Some Graduate Work", inline: true %>

  <%= f.check_box :masters_degree, label: "Master's Degree", inline: true %>

  <%= f.check_box :adv_grad_degree, label: "Advanced Graduate Degree", inline: true %>
<% end %>

<div class="form-inline">
  <div class="col-lg-3 col-xs-3 row_element">
    <strong>School Attended</strong>
  </div>

  <div class="col-lg-3 col-xs-3 row_element">
    <strong>Years</strong>
  </div>

  <div class="col-lg-3 col-xs-3 row_element">
    <strong>Major</strong>
  </div>

  <div class="col-lg-3 col-xs-3 row_element">
    <strong>Did you Graduate?</strong>
  </div>
</div>

<div class="form-inline">
  <div class="col-lg-3 col-xs-3 row_element">
    <%= f.text_field :school_attended_1,
      hide_label: true, control_col: "col-lg-12", class: "row_field" %>
  </div>
  <div class="col-lg-3 col-xs-3 row_element">
    <%= f.text_field :school_years_1,
      hide_label: true, control_col: "col-lg-12", class: "row_field" %>
  </div>
  <div class="col-lg-3 col-xs-3 row_element">
    <%= f.text_field :major_1,
      hide_label: true, control_col: "col-lg-12", class: "row_field" %>
  </div>
  <div class="col-lg-3 col-xs-3 row_element">
    <%= f.text_field :graduate_1,
      hide_label: true, control_col: "col-lg-12", class: "row_field" %>
  </div>
</div>

<div class="form-inline">
